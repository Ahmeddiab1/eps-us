BAU Retiring Vehicle Fuel Economy[Vehicle Type,Cargo Type,Vehicle Technology]=
	IF THEN ELSE(
	  Time - INITIAL TIME <= AVL Avg Vehicle Lifetime[Cargo Type,Vehicle Type] - 1,
	
	  BHNVFEAL BAU Historical New Vehicle Fuel Economy After Lifetime[Vehicle Type,Cargo Type\
		,Vehicle Technology],
	
	  BAU New Vehicle Fuel Economy After Lifetime[Vehicle Type,Cargo Type,Vehicle Technology\
		]
	)
	~	thing*miles/BTU
	~		|

Retiring Vehicle Fuel Economy[Vehicle Type,Cargo Type,Vehicle Technology]=
	IF THEN ELSE(
	  Time - INITIAL TIME <= AVL Avg Vehicle Lifetime[Cargo Type,Vehicle Type] - 1,
	
	  BHNVFEAL BAU Historical New Vehicle Fuel Economy After Lifetime[Vehicle Type,Cargo Type\
		,Vehicle Technology],
	
	  New Vehicle Fuel Economy After Lifetime[Vehicle Type,Cargo Type,Vehicle Technology\
		]
	)
	~	thing*miles/BTU
	~		|

BAU Retiring Vehicles[Vehicle Type,Cargo Type,Vehicle Technology]=
	IF THEN ELSE(
	  Time - INITIAL TIME <
	  MODULO(
	      SYVbT Start Year Vehicles by Technology[Vehicle Type,Cargo Type,Vehicle Technology\
		] / One Vehicle * One Year,
	      AVL Avg Vehicle Lifetime[Cargo Type,Vehicle Type]
	  ),
	  
	  INTEGER(
	    ZIDZ(
	      SYVbT Start Year Vehicles by Technology[Vehicle Type,Cargo Type,Vehicle Technology\
		],
	      AVL Avg Vehicle Lifetime[Cargo Type,Vehicle Type]
	    )
	  )
	  + 1,
	  
	  IF THEN ELSE(
	    Time - INITIAL TIME <= AVL Avg Vehicle Lifetime[Cargo Type,Vehicle Type] - 1,
		
		INTEGER(
	      ZIDZ(
	        SYVbT Start Year Vehicles by Technology[Vehicle Type,Cargo Type,Vehicle Technology\
		],
	        AVL Avg Vehicle Lifetime[Cargo Type,Vehicle Type]
	      )
	    ),
		
		INTEGER(
	      MIN(
	        BAU New Vehicles After Lifetime[Vehicle Type,Cargo Type,Vehicle Technology],
	        BAU Last Year Vehicles[Vehicle Type,Cargo Type,Vehicle Technology] / One Year
	      )
	    )
	  )
	)
	~	vehicles/Year
	~	Let X be the lifetime of a given type of vehicle (in years).  In the first \
		X years of the model run, we want to retire 1/X of the starting number of \
		vehicles.  Since we only work with whole numbers of vehicles (integers), \
		this may leave us with a remainder.  We deal with this by retiring the \
		remainder (if any) in the first year of the model run, in addition to the \
		1/X vehicles that are retired in each of the first X years.  After X years \
		have elapsed, we simply retire the vehicles we added to the fleet X years \
		ago.
	|

New Vehicles After Lifetime[Vehicle Type,Cargo Type,Vehicle Technology]= DELAY FIXED \
		(
	New Vehicles[Vehicle Type,Cargo Type,Vehicle Technology],
	AVL Avg Vehicle Lifetime[Cargo Type,Vehicle Type],
	0)
	~	vehicles/Year
	~		|

Additional Potential Output Needed to Meet RPS=
	MAX(
	  0,
	
	  (Elec Demand after Net Imports
	    * RPS Qualifying Percentage of Elec Demand to Seek This Year)
	  - Potential RPS Qualifying Electricity Output
	
	)
	~	MW*hour
	~		|

BAU New Vehicle Fuel Economy After Lifetime[Vehicle Type,Cargo Type,Vehicle Technology\
		]= DELAY FIXED (
	BAU New Vehicle Fuel Economy[Vehicle Type,Cargo Type,Vehicle Technology],
	AVL Avg Vehicle Lifetime[Cargo Type,Vehicle Type],
	0)
	~	thing*miles/BTU
	~		|

BAU New Vehicles After Lifetime[Vehicle Type,Cargo Type,Vehicle Technology]= DELAY FIXED\
		 (
	BAU New Vehicles[Vehicle Type,Cargo Type,Vehicle Technology],
	AVL Avg Vehicle Lifetime[Cargo Type,Vehicle Type],
	0)
	~	vehicles/Year
	~		|

BAU RPS Qualifying Share of Elec Demand after Net Imports=
	ZIDZ(
	  SUM(BAU Surviving Expected Electricity Output[Electricity Source!,Power Plant Quality\
		!]
	    * RQSD BAU RPS Qualifying Source Definitions[Electricity Source!]),
	
	  BAU Elec Demand after Net Imports
	)
	~	Dmnl
	~		|

RPS Qualifying Share of Elec Demand after Net Imports=
	ZIDZ(
	  SUM(Surviving Expected Electricity Output[Electricity Source!,Power Plant Quality!\
		]
	    * RPS Qualifying Sources[Electricity Source!]),
	
	  Elec Demand after Net Imports
	)
	~	Dmnl
	~		|

New Vehicle Fuel Economy After Lifetime[Vehicle Type,Cargo Type,Vehicle Technology]= \
		DELAY FIXED (
	New Vehicle Fuel Economy[Vehicle Type,Cargo Type,Vehicle Technology],
	AVL Avg Vehicle Lifetime[Cargo Type,Vehicle Type],
	0)
	~	thing*miles/BTU
	~		|

BAU Additional Potential Output Needed to Meet RPS=
	MAX(
	  0,
	
	  (BAU Elec Demand after Net Imports
	    * BAU RPS Qualifying Percentage of Elec Demand to Seek This Year)
	  - BAU Potential RPS Qualifying Electricity Output
	
	)
	~	MW*hour
	~		|

Do Producers Have Foresight to Future Additional RPS Values=
	IF THEN ELSE(
	  Renewable Portfolio Std Percentage
	  * Selected Policy Implementation Schedule[elec renewable portfolio standards]
	  > 0,
	
	  1,
	
	  0
	)
	~	Dmnl
	~		|

RPS Percentage by Year[Future Year]=
	MIN(
	  1,
	
	  MAX(
	    BAU RPS Percentage by Year[Future Year],
	    RPS Percentage by Year from Policy Setting[Future Year]
	  )
	)
	~	Dmnl
	~		|

Slope of Line Approaching Future RPS Percentages by Year[Future Year]=
	IF THEN ELSE(
	  Time > Numerical Future Year Definitions[Future Year],
	
	  -9999,
	
	  IF THEN ELSE(
	    Do Producers Have Foresight to Future Additional RPS Values = 0,
	
	    (BAU RPS Percentage by Year[Future Year]
	      - RPS Qualifying Share of Elec Demand after Net Imports)
	    / (Numerical Future Year Definitions[Future Year] - Time + 1) * One Year,
	
	    (RPS Percentage by Year[Future Year]
	      - RPS Qualifying Share of Elec Demand after Net Imports)
	    / (Numerical Future Year Definitions[Future Year] - Time + 1) * One Year
	  )
	)
	~	Dmnl
	~	We use -9999 for slopes of lines approaching points that are not in the \
		future, because we are going to choose the highest slope to any future \
		point, and we don't want any slopes that are not to future points to be \
		selected.  Note that we haven't built for the current model year yet in \
		the calculation flow, so the current model year counts as being in the \
		future.  Also note that in order to prevent the policy case line from \
		diverging from the BAU line in years prior to the first year policies take \
		effect (which is often a year in the past), we only give electricity \
		producers foresignt to BAU RPS values in years when the additional RPS \
		policy is not in effect (thus mirroring BAU case foresight).
	|

BAU Percentage Change in Fleet Avg Fuel Economy[Vehicle Type,Cargo Type,Vehicle Technology\
		]=
	BAU New Vehicles Contribution to Percentage Change to Fleet Avg Fuel Economy[Vehicle Type\
		,Cargo Type,Vehicle Technology]
	+ BAU Retired Vehicles Contribution to Percentage Change to Fleet Avg Fuel Economy[Vehicle Type\
		,Cargo Type,Vehicle Technology]
	~	Dmnl
	~		|

BAU Retired Vehicles Contribution to Percentage Change to Fleet Avg Fuel Economy[Vehicle Type\
		,Cargo Type,Vehicle Technology]=
	MAX(
	  0,
	
	  - BAU Percentage of Vehicle Stock Retired This Year[Vehicle Type,Cargo Type,Vehicle Technology\
		]
	  * BAU Percentage Difference in Retired Vehicle vs Fleet Avg Fuel Economy[Vehicle Type\
		,Cargo Type,Vehicle Technology]
	)
	~	Dmnl
	~	We do not allow retiring vehicles to cause fleet average fuel economy to \
		worsen, because the less-efficient vehicles are the ones that should \
		retire first, and not including this check can cause discontinuities in \
		output when the number of vehicles of a certain type and technology \
		approaches zero during the model run (for example, in a scenario with a \
		100% EV sales mandate fully ramped in by 2030, so we run out of gasoline \
		LDVs in the 2040s).
	|

Percentage Change in Fleet Avg Fuel Economy[Vehicle Type,Cargo Type,Vehicle Technology\
		]=
	New Vehicles Contribution to Percentage Change to Fleet Avg Fuel Economy[Vehicle Type\
		,Cargo Type,Vehicle Technology]
	+ Retired Vehicles Contribution to Percentage Change to Fleet Avg Fuel Economy[Vehicle Type\
		,Cargo Type,Vehicle Technology]
	~	Dmnl
	~		|

Fleet Avg Fuel Economy[Vehicle Type,Cargo Type,Vehicle Technology]=
	MAX(
	  0,
	
	  Last Year Fleet Avg Fuel Economy[Vehicle Type,Cargo Type,Vehicle Technology]
	  * (1 + Percentage Change in Fleet Avg Fuel Economy[Vehicle Type,Cargo Type,Vehicle Technology\
		])
	)
	~	thing*miles/BTU
	~		|

Retired Vehicles Contribution to Percentage Change to Fleet Avg Fuel Economy[Vehicle Type\
		,Cargo Type,Vehicle Technology]=
	MAX(
	  0,
	
	  - Percentage of Vehicle Stock Retired This Year[Vehicle Type,Cargo Type,Vehicle Technology\
		]
	  * Percentage Difference in Retired Vehicle vs Fleet Avg Fuel Economy[Vehicle Type,\
		Cargo Type,Vehicle Technology]
	)
	~	Dmnl
	~	We do not allow retiring vehicles to cause fleet average fuel economy to \
		worsen, because the less-efficient vehicles are the ones that should \
		retire first, and not including this check can cause discontinuities in \
		output when the number of vehicles of a certain type and technology \
		approaches zero during the model run (for example, in a scenario with a \
		100% EV sales mandate fully ramped in by 2030, so we run out of gasoline \
		LDVs in the 2040s).
	|

BAU Percentage Difference in New Vehicle vs Fleet Avg Fuel Economy[Vehicle Type,Cargo Type\
		,Vehicle Technology]=
	ZIDZ(
	  BAU New Vehicle Fuel Economy[Vehicle Type,Cargo Type,Vehicle Technology],
	
	  BAU Last Year Fleet Avg Fuel Economy[Vehicle Type,Cargo Type,Vehicle Technology]
	)
	- 1
	~	Dmnl
	~		|

Percentage Difference in Retired Vehicle vs Fleet Avg Fuel Economy[Vehicle Type,Cargo Type\
		,Vehicle Technology]=
	ZIDZ(
	  Retiring Vehicle Fuel Economy[Vehicle Type,Cargo Type,Vehicle Technology],
	
	  Last Year Fleet Avg Fuel Economy[Vehicle Type,Cargo Type,Vehicle Technology]
	)
	- 1
	~	Dmnl
	~		|

New Vehicles Contribution to Percentage Change to Fleet Avg Fuel Economy[Vehicle Type\
		,Cargo Type,Vehicle Technology]=
	Percentage of Vehicle Stock New This Year[Vehicle Type,Cargo Type,Vehicle Technology\
		]
	* Percentage Difference in New Vehicle vs Fleet Avg Fuel Economy[Vehicle Type,Cargo Type\
		,Vehicle Technology]
	~	Dmnl
	~		|

BAU Percentage Difference in Retired Vehicle vs Fleet Avg Fuel Economy[Vehicle Type,Cargo Type\
		,Vehicle Technology]=
	ZIDZ(
	  BAU Retiring Vehicle Fuel Economy[Vehicle Type,Cargo Type,Vehicle Technology],
	
	  BAU Last Year Fleet Avg Fuel Economy[Vehicle Type,Cargo Type,Vehicle Technology]
	)
	- 1
	~	Dmnl
	~		|

Percentage of Vehicle Stock New This Year[Vehicle Type,Cargo Type,Vehicle Technology]\
		=
	ZIDZ(
	  New Vehicles[Vehicle Type,Cargo Type,Vehicle Technology] * One Year,
	
	  Vehicles[Vehicle Type,Cargo Type,Vehicle Technology]
	)
	~	Dmnl
	~		|

Percentage of Vehicle Stock Retired This Year[Vehicle Type,Cargo Type,Vehicle Technology\
		]=
	ZIDZ(
	  Retiring Vehicles[Vehicle Type,Cargo Type,Vehicle Technology] * One Year,
	
	  Vehicles[Vehicle Type,Cargo Type,Vehicle Technology]
	)
	~	Dmnl
	~		|

BAU Percentage of Vehicle Stock Retired This Year[Vehicle Type,Cargo Type,Vehicle Technology\
