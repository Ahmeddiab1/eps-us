Transportation Fuels Taxable CO2e Emissions Intensity[Vehicle Type,Transportation Fuel\
		]=
	IF THEN ELSE(
	  Does Carbon Tax Exempt Non CO2 Emissions This Year > 0,
	
	  Transportation Fuel Pollutant Emissions Intensities This Year[Vehicle Type,CO2,Transportation Fuel\
		]
	  * GWP by Pollutant[CO2],
	
	  SUM(Transportation Fuel Pollutant Emissions Intensities This Year[Vehicle Type,Pollutant\
		!,Transportation Fuel]
	  * GWP by Pollutant[Pollutant!])
	)
	~	g CO2e/BTU
	~		|

Additional Carbon Tax on Process Emissions by Industry[Industry Category]=
	IF THEN ELSE(
	  Does Carbon Tax Exempt Process Emissions This Year > 0,
	
	  0,
	
	  QUANTUM(
	
	    IF THEN ELSE(
	      Does Carbon Tax Exempt Non CO2 Emissions This Year > 0,
	
	      Process Emissions in CO2e in Metric Tons[Industry Category,CO2]
	      * Additional Carbon Tax Rate This Year[industry sector],
	
	      SUM(Process Emissions in CO2e in Metric Tons[Industry Category,Pollutant!])
	      * Additional Carbon Tax Rate This Year[industry sector]
	    ),
	
	    QSfCF Quantization Size for Cash Flows
	  )
	)
	~	$
	~		|

