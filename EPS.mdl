Percent Change in Domestic Fuel Industry Production[natural gas and petroleum systems\
		]=
	ZIDZ(
	  SUM(
	    Percent Change in Domestic Fuel Production by Fuel[NGPS industry fuel within all fuels\
		!]
	    * BAU Fuel Production after GDP Adjustment[NGPS industry fuel within all fuels!]
	  ),
	
	  SUM(BAU Fuel Production after GDP Adjustment[NGPS industry fuel within all fuels!]\
		)
	) ~~|
Percent Change in Domestic Fuel Industry Production[coal mining]=
	ZIDZ(
	  SUM(
	    Percent Change in Domestic Fuel Production by Fuel[coal mining industry fuel within all fuels\
		!]
	    * BAU Fuel Production after GDP Adjustment[coal mining industry fuel within all fuels\
		!]
	  ),
	
	  SUM(BAU Fuel Production after GDP Adjustment[coal mining industry fuel within all fuels\
		!])
	)
	~	Dmnl
	~		|

BAU Percentage of Energy by Fuel for CCS in Industry Sector[Industrial Fuel]=
	ZIDZ(
	  SUM(BAU Industrial Fuel Use for Energy Purposes before CCS[Industry Category!,Industrial Fuel\
		]),
	
	  SUM(BAU Industrial Fuel Use for Energy Purposes before CCS[Industry Category!,Industrial Fuel\
		!])
	)
	~	Dmnl
	~	We assume that the ratios of fuels used by industry as a whole for energy \
		purposes are the same ratios of the fuels used by industry to perform CCS.  \
		This includes heat, which can be used to refresh the sorbent in some CCS \
		processes.
	|

Change in Cash Flow by Entity[Cash Flow Entity]=
	Change in Revenue by Entity[Cash Flow Entity]
	
	- Change in Energy Expenditures by Entity[Cash Flow Entity]
	- Change in Nonenergy Expenditures by Entity[Cash Flow Entity]
	~	$
	~		|

Percentage of Energy by Fuel for CCS in Industry Sector[Industrial Fuel]=
	ZIDZ(
	  SUM(Industrial Fuel Use for Energy Purposes before CCS and Methane Capture[Industrial Fuel\
		,Industry Category!]),
	
	  SUM(Industrial Fuel Use for Energy Purposes before CCS and Methane Capture[Industrial Fuel\
		!,Industry Category!])
	)
	~	Dmnl
	~	We assume that the ratios of fuels used by industry as a whole for energy \
		purposes are the same ratios of the fuels used by industry to perform CCS.  \
		This includes heat, which can be used to refresh the sorbent in some CCS \
		processes.
	|

Fraction of Heat Provided by Fuel[Target Industrial Fuel]=
	BFoHPbF BAU Fraction of Heat Provided by Fuel[Target Industrial Fuel]
	* (1 - Fraction of District Heat Fuel Use Shifted to Other Fuels This Year)
	
	+ SUM(
	    BFoHPbF BAU Fraction of Heat Provided by Fuel[Source Industrial Fuel!]
	    * Fraction of District Heat Fuel Use Shifted to Other Fuels This Year
	    * RHFF Recipient Heat Fuel Fractions[Source Industrial Fuel!,Target Industrial Fuel\
		]
	  )
	~	Dmnl
	~		|

Sum of Changes in Cash Flows=
	SUM(Change in Cash Flow by Entity[Cash Flow Entity!])
	~	$
	~		~	:SUPPLEMENTARY 
	|

Last Year Change in Revenue by Entity[Cash Flow Entity]=
	DELAY FIXED(Change in Revenue by Entity[Cash Flow Entity],
	1,
	0)
	~	$
	~		|

