	   'InputData/io-model/BPEaCP/BPEaCP-employeecomp.csv',
	   ',',
	   '1',
	   'B2'
	)
	~	$
	~		|

BAU Domestic Employment by ISIC Code[ISIC Code]=
	BPEaCP BAU Number of Employees
	* ZIDZ(
	  BEbIC BAU Employment by ISIC Code[ISIC Code],
	
	  SUM(BEbIC BAU Employment by ISIC Code[ISIC Code!])
	  )
	~	jobs
	~		|

BAU Compensation per Employee by ISIC Code[ISIC Code]=
	ZIDZ(
	  BAU Domestic Employee Compensation by ISIC Code[ISIC Code],
	
	  BAU Domestic Employment by ISIC Code[ISIC Code]
	)
	~	$/job
	~		|

BAU Domestic Employee Compensation by ISIC Code[ISIC Code]=
	BPEaCP BAU Total Employee Compensation
	* ZIDZ(
	  BECbIC BAU Employee Compensation by ISIC Code[ISIC Code],
	
	  SUM(BECbIC BAU Employee Compensation by ISIC Code[ISIC Code!])
	  )
	~	$
	~		|

BAU Weighted Average Compensation per Employee=
	ZIDZ(
	  SUM(
	    BAU Compensation per Employee by ISIC Code[ISIC Code!]
	    * BAU Domestic Employment by ISIC Code[ISIC Code!]
	  ),
	
	  SUM(BAU Domestic Employment by ISIC Code[ISIC Code!])
	)
	~	$/job
	~		|

Weighted Average Compensation per Employee=
	ZIDZ(
	  SUM(
	    Compensation per Employee by ISIC Code[ISIC Code!]
	    * Domestic Employment by ISIC Code[ISIC Code!]
	  ),
	
	  SUM(Domestic Employment by ISIC Code[ISIC Code!])
	)
	~	$/job
	~		|

