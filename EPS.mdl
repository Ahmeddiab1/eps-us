Domestic Employee Compensation=
	BPEaCP BAU Total Employee Compensation
	+ SUM(Change in Domestic Employee Compensation by ISIC Code[ISIC Code!])
	~	$
	~		|

Domestic Employee Compensation by ISIC Code[ISIC Code]=
	Domestic Employee Compensation
	* ZIDZ(
	    Domestic Employee Compensation by ISIC Code without BAU Projection[ISIC Code],
	    SUM(Domestic Employee Compensation by ISIC Code without BAU Projection[ISIC Code\
		!])
	  )
	~	$
	~		|

BPEaCP Population:=
	GET DIRECT DATA(
	   'InputData/io-model/BPEaCP/BPEaCP-population.csv',
	   ',',
	   '1',
	   'B2'
	)
	~	people
	~		|

New EV Chargers This Year=
	(EV Charger Deployment To Date
	  - Last Year EV Charger Deployment To Date)
	/ People per Hundred Thousand People
	* BPEaCP Population
	~	chargers
	~		|

Domestic Employment=
	BPEaCP BAU Number of Employees
	+ SUM(Change in Domestic Jobs after Productivity Effects by ISIC Code[ISIC Code!])
	~	jobs
	~		|

Domestic Employment by ISIC Code[ISIC Code]=
	Domestic Employment
	* ZIDZ(
	    Domestic Employment by ISIC Code without BAU Projection[ISIC Code],
	    SUM(Domestic Employment by ISIC Code without BAU Projection[ISIC Code!])
	  )
	~	jobs
	~		|

Compensation per Employee by ISIC Code[ISIC Code]=
	ZIDZ(
	  Domestic Employee Compensation by ISIC Code[ISIC Code],
	
	  Domestic Employment by ISIC Code[ISIC Code]
	)
	~	$/job
	~		|

BPEaCP BAU Number of Employees:=
	GET DIRECT DATA(
	   'InputData/io-model/BPEaCP/BPEaCP-employedpop.csv',
	   ',',
	   '1',
	   'B2'
	)
	~	jobs
	~		|

BPEaCP BAU Total Employee Compensation:=
