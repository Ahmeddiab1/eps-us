	ZIDZ(
	  (SUM(Last Year Electricity Generation Capacity[solar PV es,Power Plant Quality!])
	     + SUM(BDEQ BAU Distributed Electricity Source Capacity[Building Type!,solar PV es\
		])
	     + SUM(Cumulative Total Distributed Solar Capacity Caused by Policies[Building Type\
