"New Components Potential Energy Use after Standards and R&D"[Building Type,Building Fuel\
		,Building Component]=
	MAX(
	  0,
	
	  BAU New Components Energy Use[Building Type,Building Fuel,Building Component]
	  * (1 - Reduction in E Use Allowed by Component Eff Std This Year[Building Component\
		,Building Type])
	  * (1 - RnD Building Fuel Use Perc Reduction This Year[Building Component])
	)
	~	BTU
	~		|

Retiring Components Potential Energy Use[Building Type,Building Fuel,Building Component\
		]=
	MAX(
	  0,
	
	  MIN(
	    Last Year Components Potential Energy Use[Building Type,Building Fuel,Building Component\
		]
	    / One Year,
	
	    IF THEN ELSE(
	      Time < INITIAL TIME + CL Component Lifetime[Building Type,Building Component],
	
	      ZIDZ(
	        SYCEU Start Year Components Energy Use[Building Type,Building Fuel,Building Component\
		],
	        CL Component Lifetime[Building Type,Building Component]
	      ),
	
	      New Components Potential Energy Use after Envelope Effects after Lifetime[Building Type\
		,Building Fuel,Building Component]
	    )
	  )
	)
	~	BTU/Year
	~		|

Perc Change in New Component Energy Use per Unit Floor Area[Building Type,Building Fuel\
		,Building Component]=
	ZIDZ(
	  New Components Potential Energy Use after Quality Level Shifts[Building Type,Building Fuel\
		,Building Component]
	  - BAU New Components Energy Use[Building Type,Building Fuel,Building Component],
	
	  BAU New Components Energy Use[Building Type,Building Fuel,Building Component]
	)
	~	Dmnl
	~		|

BAU Retiring Components Energy Use[Building Type,Building Fuel,Building Component]=
	MAX(
	  0,
	
	  MIN(
	    BAU Last Year Components Energy Use[Building Type,Building Fuel,Building Component\
		]
	    / One Year,
	
	    IF THEN ELSE(
	      Time < INITIAL TIME + CL Component Lifetime[Building Type,Building Component],
	
	      ZIDZ(
	        SYCEU Start Year Components Energy Use[Building Type,Building Fuel,Building Component\
		],
	        CL Component Lifetime[Building Type,Building Component]
	      ),
	
	      BAU New Components Energy Use after Envelope Effects after Lifetime[Building Type\
		,Building Fuel,Building Component]
	    )
	  )
	)
	~	BTU/Year
	~		|

